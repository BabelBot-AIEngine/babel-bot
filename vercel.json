{
  "functions": {
    "api/index.js": {
      "maxDuration": 300
    },
    "api/webhooks/index.ts": {
      "maxDuration": 30
    },
    "api/tasks/enhanced/[taskId]/retrigger-webhook.ts": {
      "maxDuration": 30
    }
  },
  "outputDirectory": "dist/client",
  "rewrites": [
    {
      "source": "/api/webhooks",
      "destination": "/api/webhooks/index.ts"
    },
    {
      "source": "/api/webhooks/:state/:taskId",
      "destination": "/api/webhooks/index.ts"
    },
    {
      "source": "/api/webhooks/:state/:taskId/:language",
      "destination": "/api/webhooks/index.ts"
    },
    {
      "source": "/api/webhooks/:state/:taskId/:language/:iteration",
      "destination": "/api/webhooks/index.ts"
    },
    {
      "source": "/api/tasks/enhanced/([^/]+)/retrigger-webhook",
      "destination": "/api/tasks/enhanced/[taskId]/retrigger-webhook.ts"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    }
  ]
}
