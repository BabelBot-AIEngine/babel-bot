{
  "functions": {
    "api/index.js": {
      "maxDuration": 300
    },
    "api/webhooks/index.ts": {
      "maxDuration": 30
    },
    "api/webhook/[state]/[taskId].ts": {
      "maxDuration": 30
    },
    "api/state/translate.ts": {
      "maxDuration": 30
    },
    "api/state/verify.ts": {
      "maxDuration": 30
    },
    "api/state/review.ts": {
      "maxDuration": 30
    },
    "api/state/finalize.ts": {
      "maxDuration": 30
    },
    "api/tasks/enhanced/[taskId]/retrigger-webhook.ts": {
      "maxDuration": 30
    }
  },
  "outputDirectory": "dist/client",
  "rewrites": [
    {
      "source": "/api/webhooks",
      "destination": "/api/webhooks/index.ts"
    },
    {
      "source": "/api/webhook/(.*)/(.*)",
      "destination": "/api/webhook/[state]/[taskId].ts"
    },
    {
      "source": "/api/state/translate",
      "destination": "/api/state/translate.ts"
    },
    {
      "source": "/api/state/verify",
      "destination": "/api/state/verify.ts"
    },
    {
      "source": "/api/state/review",
      "destination": "/api/state/review.ts"
    },
    {
      "source": "/api/state/finalize",
      "destination": "/api/state/finalize.ts"
    },
    {
      "source": "/api/tasks/enhanced/([^/]+)/retrigger-webhook",
      "destination": "/api/tasks/enhanced/[taskId]/retrigger-webhook.ts"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    }
  ]
}
